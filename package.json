{
  "name": "@picturelink/mot-check",
  "version": "0.0.4",
  "description": "A simple javascript library for accessing the DVLA MOT Check API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "barrels": "barrelsby --config barrelsby.json",
    "lint": "eslint -c .eslintrc.json --ext .ts src",
    "clean": "shx rm -Rf dist",
    "prebuild": "npm run barrels && npm run lint",
    "build": "tsc",
    "rebuild": "npm run clean && npm run build",
    "test": "mocha -r ts-node/register tests/**/*.spec.ts",
    "gendoc": "jsdoc2md -f ./dist/**/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/picturelink/dvla-mot-check-js"
  },
  "author": "John Bayly",
  "license": "MIT",
  "homepage": "https://github.com/picturelink/dvla-mot-check-js",
  "keywords": [
    "TypeScript",
    "DVLA",
    "MOT"
  ],
  "engines": {
    "node": ">=10"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/chai-as-promised": "^7.1.2",
    "@types/mocha": "^7.0.2",
    "@types/sinon": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@typescript-eslint/parser": "^2.31.0",
    "barrelsby": "^2.2.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^6.8.0",
    "jsdoc-to-markdown": "^5.0.3",
    "mocha": "^7.1.2",
    "shx": "^0.3.2",
    "sinon": "^9.0.2",
    "ts-node": "^8.10.1",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@types/node-fetch": "^2.5.7",
    "node-fetch": "^3.1.1",
    "query-string": "^6.12.1"
  },
  "files": [
    "dist/**/*"
  ]
}
